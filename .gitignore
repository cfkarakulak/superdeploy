# =============================================================================
# SuperDeploy - Git Ignore
# =============================================================================

# Environment & Secrets
.env
.env.*
*.env.local
*.env.*.local
secrets.env
projects/*/secrets.env
projects/*/compose/env/*.env
!projects/*/compose/env/*.env.example

# Generated passwords (NEVER commit!)
projects/*/.passwords.yml
.passwords.yml

# Merged secrets (temporary files for debugging)
projects/*/.merged-secrets-*.yml

# Python
__pycache__/
*.py[cod]
*$py.class
*.so
.Python
venv/
env/
ENV/
*.egg-info/
dist/
build/
*.egg

# Terraform
**/.terraform/
**/.terraform.lock.hcl
*.tfstate
*.tfstate.backup
*.tfstate.*
terraform/envs/**/*.tfvars
shared/terraform/envs/**/*.tfvars
!terraform/envs/**/*.tfvars.example
!shared/terraform/envs/**/*.tfvars.example

# Ansible
ansible/*.retry
ansible/*.log
ansible/.ansible/

# IDE
.vscode/
.idea/
*.swp
*.swo
*~
.DS_Store

# Logs
*.log
logs/
*.out

# Backups
*.bak
*.backup
*.old

# SSH Keys (never commit!)
*.pem
*.key
id_rsa*
!*.pub

# Temporary files
tmp/
temp/
.cache/
