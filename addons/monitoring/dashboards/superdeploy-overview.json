{
  "title": "SuperDeploy - Overview",
  "tags": [
    "superdeploy",
    "overview"
  ],
  "timezone": "browser",
  "schemaVersion": 16,
  "version": 1,
  "refresh": "30s",
  "panels": [
    {
      "id": 1,
      "title": "Total Projects",
      "type": "stat",
      "gridPos": {
        "h": 4,
        "w": 6,
        "x": 0,
        "y": 0
      },
      "targets": [
        {
          "expr": "count(count by (project) (up))",
          "refId": "A",
          "legendFormat": ""
        }
      ],
      "options": {
        "colorMode": "value",
        "graphMode": "none",
        "textMode": "value",
        "reduceOptions": {
          "values": false,
          "calcs": [
            "lastNotNull"
          ]
        }
      },
      "fieldConfig": {
        "defaults": {
          "color": {
            "mode": "thresholds"
          },
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {
                "value": null,
                "color": "green"
              }
            ]
          }
        }
      }
    },
    {
      "id": 2,
      "title": "Services Up",
      "type": "stat",
      "gridPos": {
        "h": 4,
        "w": 6,
        "x": 6,
        "y": 0
      },
      "targets": [
        {
          "expr": "sum(up == 1)",
          "refId": "A",
          "legendFormat": ""
        }
      ],
      "options": {
        "colorMode": "value",
        "graphMode": "area",
        "textMode": "value",
        "reduceOptions": {
          "values": false,
          "calcs": [
            "lastNotNull"
          ]
        }
      },
      "fieldConfig": {
        "defaults": {
          "color": {
            "mode": "thresholds"
          },
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {
                "value": null,
                "color": "red"
              },
              {
                "value": 1,
                "color": "green"
              }
            ]
          }
        }
      }
    },
    {
      "id": 3,
      "title": "Services Down",
      "type": "stat",
      "gridPos": {
        "h": 4,
        "w": 6,
        "x": 12,
        "y": 0
      },
      "targets": [
        {
          "expr": "sum(up == 0)",
          "refId": "A",
          "legendFormat": ""
        }
      ],
      "options": {
        "colorMode": "value",
        "graphMode": "area",
        "textMode": "value",
        "reduceOptions": {
          "values": false,
          "calcs": [
            "lastNotNull"
          ]
        }
      },
      "fieldConfig": {
        "defaults": {
          "color": {
            "mode": "thresholds"
          },
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {
                "value": null,
                "color": "green"
              },
              {
                "value": 1,
                "color": "red"
              }
            ]
          }
        }
      }
    },
    {
      "id": 4,
      "title": "Total HTTP Requests (5m)",
      "type": "stat",
      "gridPos": {
        "h": 4,
        "w": 6,
        "x": 18,
        "y": 0
      },
      "targets": [
        {
          "expr": "sum(rate(caddy_http_requests_total[5m])) * 300",
          "refId": "A",
          "legendFormat": ""
        }
      ],
      "options": {
        "colorMode": "value",
        "graphMode": "area",
        "textMode": "value",
        "reduceOptions": {
          "values": false,
          "calcs": [
            "lastNotNull"
          ]
        }
      }
    },
    {
      "id": 5,
      "title": "Service Status by Project",
      "type": "table",
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 0,
        "y": 4
      },
      "targets": [
        {
          "expr": "up",
          "refId": "A",
          "format": "table",
          "instant": true
        }
      ],
      "transformations": [],
      "options": {
        "showHeader": true
      },
      "fieldConfig": {
        "overrides": [
          {
            "matcher": {
              "id": "byName",
              "options": "Status"
            },
            "properties": [
              {
                "id": "mappings",
                "value": [
                  {
                    "type": "value",
                    "value": "0",
                    "text": "Down",
                    "color": "red"
                  },
                  {
                    "type": "value",
                    "value": "1",
                    "text": "Up",
                    "color": "green"
                  }
                ]
              }
            ]
          }
        ]
      }
    },
    {
      "id": 6,
      "title": "HTTP Request Rate by Project",
      "type": "timeseries",
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 12,
        "y": 4
      },
      "targets": [
        {
          "expr": "sum by (project) (rate(caddy_http_requests_total[5m]))",
          "refId": "A",
          "legendFormat": "{{project}}"
        }
      ],
      "options": {
        "legend": {
          "displayMode": "table",
          "placement": "right",
          "calcs": [
            "mean",
            "lastNotNull"
          ]
        }
      },
      "fieldConfig": {
        "defaults": {
          "custom": {
            "drawStyle": "line",
            "lineInterpolation": "smooth",
            "fillOpacity": 10
          },
          "unit": "reqps"
        }
      }
    },
    {
      "id": 7,
      "title": "Response Time (p95) by Project",
      "type": "timeseries",
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 0,
        "y": 12
      },
      "targets": [
        {
          "expr": "histogram_quantile(0.95, sum by (project, le) (rate(caddy_http_request_duration_seconds_bucket[5m])))",
          "refId": "A",
          "legendFormat": "{{project}}"
        }
      ],
      "options": {
        "legend": {
          "displayMode": "table",
          "placement": "right",
          "calcs": [
            "mean",
            "max"
          ]
        }
      },
      "fieldConfig": {
        "defaults": {
          "custom": {
            "drawStyle": "line",
            "lineInterpolation": "smooth",
            "fillOpacity": 10
          },
          "unit": "s",
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {
                "value": null,
                "color": "green"
              },
              {
                "value": 0.5,
                "color": "yellow"
              },
              {
                "value": 1,
                "color": "red"
              }
            ]
          }
        }
      }
    },
    {
      "id": 8,
      "title": "Error Rate by Project",
      "type": "timeseries",
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 12,
        "y": 12
      },
      "targets": [
        {
          "expr": "sum by (project) (rate(caddy_http_requests_total{status=~\"5..\"}[5m])) / sum by (project) (rate(caddy_http_requests_total[5m])) * 100",
          "refId": "A",
          "legendFormat": "{{project}}"
        }
      ],
      "options": {
        "legend": {
          "displayMode": "table",
          "placement": "right",
          "calcs": [
            "mean",
            "max"
          ]
        }
      },
      "fieldConfig": {
        "defaults": {
          "custom": {
            "drawStyle": "line",
            "lineInterpolation": "smooth",
            "fillOpacity": 10
          },
          "unit": "percent",
          "thresholds": {
            "mode": "absolute",
            "steps": [
              {
                "value": null,
                "color": "green"
              },
              {
                "value": 1,
                "color": "yellow"
              },
              {
                "value": 5,
                "color": "red"
              }
            ]
          }
        }
      }
    }
  ]
}