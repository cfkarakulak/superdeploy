# Forgejo Actions Runner Configuration
# Generated by Ansible from project configuration

log:
  level: info

runner:
  file: .runner
  capacity: {{ runner_capacity | default(1) }}
  timeout: {{ runner_timeout | default('3h') }}
  insecure: false
  fetch_timeout: 5s
  fetch_interval: 2s
  labels: []

cache:
  enabled: true
  dir: ""
  host: ""
  port: 0
  external_server: ""

container:
  network: ""
  privileged: false
  options: ""
  workdir_parent: ""
  valid_volumes: []
  docker_host: ""
  force_pull: false

host:
  workdir_parent: "{{ runner_workdir_parent | default('/opt/superdeploy') }}"
